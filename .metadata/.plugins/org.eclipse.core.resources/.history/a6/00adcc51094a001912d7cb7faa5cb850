import java.util.ArrayList;
import java.util.List;

public class TopologySort {

	static final int V = 7; // Á¤Á¡ 7°³

	public static void main(String[] args) {

		List<Integer>[] list = new ArrayList[V + 1];

		for (int i = 1; i <= V; i++)
			list[i] = new ArrayList<Integer>();

		list[1].add(2);
		list[1].add(5);
		list[2].add(3);
		list[3].add(4);
		list[4].add(6);
		list[5].add(6);
		list[6].add(7);
	}

}
