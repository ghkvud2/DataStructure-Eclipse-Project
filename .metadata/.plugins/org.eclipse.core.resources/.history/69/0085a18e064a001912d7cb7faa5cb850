public class TopologySort {

	static final int VERTEX = 7, EDGE = 9;
	static List<Integer>[] list;

	public static void main(String[] args) {

		list = new ArrayList[VERTEX];
		list[1].add(2);
		list[1].add(5);
		list[2].add(3);
		list[3].add(4);
		list[4].add(6);
		list[5].add(6);
		list[6].add(7);

	}

}
